# .env.prod.example
# Copie para .env.prod e ajuste os valores de producao.
GOOGLE_APPLICATION_CREDENTIALS="/path/to/credentials/sonnda-gcs.json"
GCP_PROJECT_ID=sonnda
GCP_PROJECT_NUMBER=project-number

# Firebase (Web SDK - Client Side)
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_API_KEY=your-web-api-key
FIREBASE_APP_ID=your-app-id

# Storage
GCS_BUCKET=bucket-name
GCP_LOCATION=location

# Document AI
GCP_EXTRACT_LABS_PROCESSOR_ID=processor_id

# Supabase configuration
SUPABASE_URL=supabase-connection-string

# Redis configuration
REDIS_URL=redis-rest-url

# Application configuration
APP_ENV=prod
LOG_LEVEL=info
LOG_FORMAT=json
APP_HOST=app.sonnda.com.br
API_HOST=api.sonnda.com.br
PORT=8080
